{
  "name": "react-quick-start",
  "version": "1.0.0",
  "description": "react快速开始项目，使用parcel构建",
  "main": "src/index.js",
  "scripts": {
    "start": "rimraf dist && parcel src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build src/index.js -d build/output --public-url ./"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "less": "^2.7.3",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "parcel-bundler": "^1.9.7",
    "postcss-modules": "^1.3.2",
    "rimraf": "^2.6.2"
  },
  "postcss": {
    "modules": true,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009",
        "grid": true
      }
    }
  }
}
